<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_file.xsd">
	<system>
		<tab id="theme" sortOrder="999" translate="label">
			<label>theme</label>
		</tab>
		<section id="strip" showInDefault="1" showInStore="1" showInWebsite="1" sortOrder="10" translate="label">
			<label>head strip</label>
			<tab>theme</tab>
			<resource>Shulgin_Headerstrip::config_shulgin_headerstrip</resource>
			<group id="general" showInDefault="1" showInStore="1" showInWebsite="1" sortOrder="10" translate="label">
				<label>general</label>
				<field id="is_active" showInDefault="1" showInStore="1" showInWebsite="1" sortOrder="10" translate="label" type="select">
					<label>is_active</label>
					<comment/>
					<source_model>Magento\Config\Model\Config\Source\Enabledisable</source_model>
				</field>
			</group>
		</section>
	</system>
</config>
